{
  "name": "@am92/mongo-odm",
  "version": "1.0.6",
  "description": "Mongo DB ODM (Object Document Mapper)",
  "types": "module",
  "main": "dist/MongoModel.js",
  "module": "src/MongoModel.mjs",
  "scripts": {
    "test": "jest",
    "build": "rimraf dist/ && babel src/ --out-dir dist/ --copy-files",
    "preversion": "pnpm test",
    "version": "pnpm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "repository": "git@github.com:heliumtank92/am92-mongo-odm.git",
  "homepage": "https://github.com/heliumtank92/am92-mongo-odm#readme",
  "contributors": [
    {
      "name": "Ankit Gandhi",
      "email": "ankitgandhi452@gmail.com",
      "url": "https://github.com/ankitgandhi452"
    },
    {
      "name": "Mehul Agarwal",
      "email": "agarwalmehul92@gmail.com",
      "url": "https://github.com/agarwalmehul"
    }
  ],
  "license": "MIT",
  "exports": {
    "./mongoConnect": {
      "require": "./dist/mongoConnect.js",
      "default": "./src/mongoConnect.mjs"
    },
    "./mongoSchemaWrapper": {
      "require": "./dist/mongoSchemaWrapper.js",
      "default": "./src/mongoSchemaWrapper.mjs"
    },
    "./MongoModel": {
      "require": "./dist/MongoModel.js",
      "default": "./src/MongoModel.mjs"
    },
    "./MongoError": {
      "require": "./dist/MongoError.js",
      "default": "./src/MongoError.mjs"
    },
    "./mongoose": {
      "require": "./dist/mongoose.js",
      "default": "./src/mongoose.mjs"
    }
  },
  "dependencies": {
    "@am92/api-logger": "^1.0.5",
    "mongoose": "^6.8.1",
    "mongoose-lean-virtuals": "^0.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.3.1",
    "babel-plugin-module-extension": "^0.1.3",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2"
  },
  "keywords": [
    "mongo",
    "mongo odm",
    "mongo db",
    "mongoose",
    "mongoose wrapper",
    "documentdb",
    "nosql",
    "document"
  ]
}
